{
  "mcpServers": {
    "filesystem": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--mount",
        "type=bind,src=d:/code/HumanModelGeneration,dst=/projects/HumanModelGeneration",
        "mcp/filesystem",
        "/projects"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "browser-tools": {
      "command": "npx",
      "args": [
        "@agentdeskai/browser-tools-mcp@latest"
      ],
      "disabled": false,
      "autoApprove": [
        "getNetworkLogs",
        "getConsoleLogs",
        "getConsoleErrors",
        "getNetworkErrors"
      ]
    },
    "tavily": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "TAVILY_API_KEY=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",
        "mcp/tavily:latest"
      ],
      "disabled": false,
      "autoApprove": [
        "tavily-search",
        "tavily-extract",
        "tavily-crawl",
        "tavily-map"
      ]
    },
    "sequentialthinking": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "mcp/sequentialthinking"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "fetch": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "-e",
        "HTTP_PROXY=http://host.docker.internal:30080",
        "-e",
        "HTTPS_PROXY=http://host.docker.internal:30080",
        "mcp/fetch:latest",
        "--ignore-robots-txt"
      ],
      "disabled": false,
      "autoApprove": [
        "fetch"
      ]
    }
  }
}
